import java.util.Scanner;

public class ReverseNum {

	public int rev(int n)
	{
		int sum=0;
	while(n>0) {
		int r=n%10;
		sum=sum*10+r;
		n=n/10;
	}
	return sum ;
	}
	public static void main(String[] args) {
		Scanner sc= new Scanner(System.in);
		int num = sc.nextInt();
		ReverseNum r = new ReverseNum();
		r.rev(num);

	}
}
