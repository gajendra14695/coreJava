import java .util.Scanner;
public class Sod {
public static void main(String[] args) {
	Scanner sc = new Scanner(System.in);
	int n = sc.nextInt();
	System.out.print("sum is ");
	for(int i=1;i<n;i++) {

int sum=0;
if(n-1>=i) {
sum =i*(i+1)/2;
		 System.out.print(sum+",");
	}
	else
	{
		sum=i*(i+i)/2;
		System.out.print(sum+" ");
		
	}
	}
}
}
