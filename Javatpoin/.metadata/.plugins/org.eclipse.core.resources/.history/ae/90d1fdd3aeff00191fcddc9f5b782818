			ht.put(102, "vijay");
			ht.put(100, "sonu");
			ht.put(99, "rahul");
			ht.put(101, "monu");		
			//ht.put(null, "john");
			// ht.put(103, null);
			 
				System.out.println("Data "+ht);
		
		}
}
