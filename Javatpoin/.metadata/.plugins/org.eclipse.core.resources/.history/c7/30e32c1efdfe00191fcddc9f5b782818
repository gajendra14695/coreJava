package com.ustglobal.sorting.set;

import java.util.Iterator;
import java.util.TreeSet;

public class TestL {

		
		public static void main(String[] args) {
			
			TreeSet<Customer> ts = new TreeSet<Customer>(sp);

			ts.add(new Customer(1,"gajendra",678908));
			ts.add(new Customer(2,"Sumit",56568908));
			ts.add(new Customer(3, "Ranjeet",90678908));
			ts.add(new Customer(4,"Amrit",3478908));
			//ts.add(new CustomerBank("CANARA",56004,3478908));

			System.out.println("======================================");

			Iterator<Bank> it = ts.iterator();
			while(it.hasNext()) {
				Bank e = it.next();
				System.out.println("ID is "+e.pincode);
				System.out.println("name is "+e.name);
				System.out.println("Salary is "+e.micr);
				System.out.println("=============================================");

			}
		}
}
